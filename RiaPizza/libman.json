{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "angular.js@1.7.8",
      "destination": "wwwroot/lib/angular.js/",
      "files": [
        "angular.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "angular-file-upload@2.5.0",
      "destination": "wwwroot/lib/angular-file-upload/"
    }
  ]
}